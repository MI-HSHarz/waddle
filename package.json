{
  "name": "waddle",
  "version": "0.0.1",
  "description": "",
  "author": "waddle",
  "license": "no license yet",
  "main": "main.js",
  "repository": "https://github.com/MI-HSHarz/waddle",
  "scripts": {
    "start": "electron app",
    "prestart":"webpack --profile --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "dev-server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "typings": "typings",
    "lint": "tsconfig-lint",
    "postinstall": "typings install",
    "electron": "electron app",
    "package-all": "electron-packager ./app waddle --out=Staging --platform=all --arch=all --ignore=src --ignore=bootstrap.ts --ignore=vendor.ts --overwrite",
    "package-mac": "electron-packager ./app waddle --out=Staging --platform=mas --arch=all --ignore=src --ignore=bootstrap.ts --ignore=vendor.ts --overwrite",
    "package-win": "electron-packager ./app waddle --out=Staging --platform=win32 --arch=x64 --asar --prune  --arch=x64  --ignore=src --ignore=bootstrap.ts --ignore=vendor.ts"

  },
  "devDependencies": {
    "css-loader": "0.23.1",
    "electron-packager": "7.0.3",
    "electron-prebuilt": "1.2.1",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "node-sass": "3.7.0",
    "raw-loader": "0.5.1",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tsconfig-lint": "0.11.0",
    "tslint": "3.10.2",
    "tslint-loader": "2.1.4",
    "typedoc": "0.3.12",
    "typescript": "1.8.10",
    "typings": "0.8.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "zone.js": "0.6.10",
    "gulp": "3.9.1",
    "es6-shim": "0.35.0",
    "es6-promise": "3.1.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2"
  }
}
